{
  "name": "@coopetition/gs-doc-cache",
  "version": "1.0.0",
  "description": "OpenTTD GameScript API documentation caching tool",
  "main": "gs-doc-cache.mjs",
  "type": "module",
  "bin": {
    "gs-doc-cache": "./gs-doc-cache.mjs",
    "update-gs-cache": "./bin/update-cache.mjs"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "openttd",
    "gamescript",
    "api",
    "documentation",
    "cache"
  ],
  "author": "Coopetition Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahulneel/openttd-coopetition.git",
    "directory": "workspaces/gs-doc-cache"
  },
  "bugs": {
    "url": "https://github.com/bahulneel/openttd-coopetition/issues"
  },
  "homepage": "https://github.com/bahulneel/openttd-coopetition#readme",
  "dependencies": {
    "cheerio": "^1.1.2",
    "fuse.js": "^7.1.0"
  },
  "devDependencies": {
    "eslint": "^9.35.0"
  },
  "files": [
    "gs-doc-cache.mjs",
    "bin/",
    "README.md"
  ],
  "scripts": {
