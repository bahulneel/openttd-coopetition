{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.associations": {
    "*.nut": "plaintext"
  },
  "eslint.enable": true,
  "eslint.useFlatConfig": true,
  "eslint.validate": ["javascript", "javascriptreact", "plaintext"],
  "eslint.options": {
    "extensions": [".nut"]
  },
  "eslint.workingDirectories": [
    "./"
  ],
  "cSpell.words": [
    "Coopetition"
  ]
}